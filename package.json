{
  "name": "task_opad",
  "version": "1.0.0",
  "description": "TaskOpad",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start": "tsc --watch & nodemon --inspect dist",
    "build": "tsc && mkdir -p uploads && mkdir -p dist/templates && mkdir -p dist/assets && cp -R src/templates/* dist/templates && cp -R src/assets/* dist/assets",
    "build-dev": "tsc && mkdir -p uploads && mkdir -p dist/templates && mkdir -p dist/assets && cp -R src/templates/* dist/templates && cp -R src/assets/* dist/assets",
    "build-stage": "tsc && mkdir -p uploads && mkdir -p dist/templates && mkdir -p dist/assets && cp -R src/templates/* dist/templates && cp -R src/assets/* dist/assets",
    "build-prod": "tsc && mkdir -p uploads && mkdir -p dist/templates && mkdir -p dist/assets && cp -R src/templates/* dist/templates && cp -R src/assets/* dist/assets",
    "env": "sync-dotenv --sample env.sample"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run env",
      "commit-msg": ".githooks/commit-msg ${HUSKY_GIT_PARAMS}"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://git.sa-labs.info/taskopad/taskopad-apis-nodejs.git"
  },
  "author": "Sachin Suthariya",
  "license": "ISC",
