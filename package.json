    "@types/stripe": "^7.13.23",
    "awesome-phonenumber": "^2.35.0",
    "aws-sdk": "^2.713.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "city-timezones": "^1.2.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "compression": "^1.7.3",
    "connect-busboy": "0.0.2",
    "crypto": "^1.0.1",
